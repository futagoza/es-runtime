{
    "name": "es-runtime",
    "version": "1.1.0",
    "description": "ensure @babel/(runtime && polyfill) are installed",
    "keywords": [
        "babel",
        "runtime",
        "polyfill",
        "core-js",
        "regenerator"
    ],
    "repository": "futagoza/es-runtime",
    "bugs": "https://github.com/futagoza/es-runtime/issues",
    "license": "MIT",
    "author": "Futago-za Ryuu",
    "files": [
        "polyfill.js",
        "postinstall.js"
    ],
    "main": "polyfill.js",
    "bin": "postinstall.js",
    "scripts": {
        "postinstall": "node postinstall.js",
        "lint": "eslint *.js test/*.js",
        "test": "rimraf test/node_modules && node postinstall --module test && node test/assert",
        "postversion": "git push --follow-tags && npm publish"
    },
    "dependencies": {
        "@babel/polyfill": "latest",
        "@babel/runtime": "latest",
        "semver": "latest"
    },
    "devDependencies": {
        "rimraf": "latest",
        "eslint": "latest",
        "eslint-config-futagozaryuu": "latest"
    },
    "requiredDependencies": [
        "@babel/runtime",
        "@babel/polyfill",
        "core-js",
        "regenerator-runtime"
    ],
    "engines": {
        "node": ">= 4"
    }
}
